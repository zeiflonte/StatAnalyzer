        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stats"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,0.4,-1.2">
        <Canvas x:Name="canvas" Margin="0,111,0,0">
            
        </Canvas>
        <Button x:Name="butLoad" Content="Load file" HorizontalAlignment="Left" Margin="224,35,0,0" VerticalAlignment="Top" Width="75" Click="butLoad_Click"/>
        <Button x:Name="btnStart" Content="Process" Background="LightBlue" Margin="278,75,119.6,214.2" Click="btnStart_Click"></Button>
        <Button x:Name="btnClearData" Content="Clear data" Background="LightBlue" Margin="125,75,273.6,214.2" Click="btnClearData_Click"/>
    </Grid>
</Window>
