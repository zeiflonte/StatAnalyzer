<Window x:Class="Stats.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stats"
        mc:Ignorable="d"
        Title="MainWindow" Height="542.692" Width="525">
    <Grid Margin="0,0,0.4,-1.2">
        <Canvas x:Name="canvas" Margin="0,146,-0.4,183.2" Background="#FFE2F3E3"/>
        <Button x:Name="butLoad" Content="Load file" HorizontalAlignment="Left" Margin="223,36,0,0" VerticalAlignment="Top" Width="75" Click="butLoad_Click"/>
        <Button x:Name="btnStart" Content="Process" HorizontalAlignment="Left" Background="LightBlue" Margin="323,76,0,406.2" Width="75" Click="btnStart_Click"></Button>
        <Button x:Name="btnClearData" Content="Clear data" HorizontalAlignment="Left" Background="LightBlue" Margin="124,76,0,406.2" Width="75" Click="btnClearData_Click"/>
        <Canvas x:Name="canvasContig" Margin="0,330,-0.4,0.2" Background="#FFFFDCDC"/>
    </Grid>
</Window>
